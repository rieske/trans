
int MAXLINE = 255;


int write_out(char *type/*abc*/, void *out)
{
    if (type == "%s" )    // char array
    {
        char *string = (char*)out;
        while (/* comment//*/*string != '\0')
            printf("%d", *string++);
        return 1;
    }
    else if (type == "%d")
    {
        int *integer = (int*)out;
        printf("%d", *integer);
        return 1;
    }
    return 0;
}

int getLine(char *line)
{
    char c;
    int i = 0;
    while ( (c != '\n') && /*/* comment */(i < MAXLINE-2) )
    {
        scanf("%d", &c);
        *line++ = c;
        ++i;
    }

    *line++ = '\n';
    *line = '\0';

    if (i == MAXLINE)
        return 0;
    else
        return 1;
}

int countSomething()
{
    int a, b, c, d;
    a = 2;
    b = 5;
    c = 9;
    d = 1;
    return -(a * 2-5*d+a -d*-b-b-b) * 2 +c;
}

int main()
{
    char *str= "Hello World!\n";
    char c = 'c';
    char nl = '\n';

    char lineIn[MAXLINE];

    int retVal;

    retVal = write_out("%s", str);       // eol comment
    write_out("%d", retVal);

    if (getLine(lineIn))
        write_out("%s", lineIn);

    write_out("%d", countSomething());
    return 0;
}
